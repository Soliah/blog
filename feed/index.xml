---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>クリス</title>
 <link href="http://chris.chowie.net/feed/index.xml" rel="self"/>
 <link href="http://chris.chowie.net/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://chris.chowie.net/</id>
 <author>
   <name>Christopher Chow</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://chris.chowie.net{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://chris.chowie.net{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
